<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Created</th>
      <th>Last updated</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.created_at %></td>
        <td><%= user.updated_at %></td>
        <td><%= user.admin %></td>
        <td><%= link_to 'Show', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Back', ideas_path %>
